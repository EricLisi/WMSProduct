@{
    ViewBag.Title = "Form";
    Layout = "~/Views/Shared/_Form.cshtml";
}

<form id="form1">
    <div class="form row">
        <div class="col-md-6 col-sm-6 col-xs-12">
            <table>
                <tr>
                    <th class="formTitle">编码</th>
                    <td class="formValue">
                        <input id="F_EnCode" name="F_EnCode" type="text" class="form-control required" placeholder="请输入编码" />
                    </td>
                </tr>
            </table>
        </div>
        <div class="col-md-6 col-sm-6 col-xs-12">
            <table>
                <tr>
                    <th class="formTitle">名称</th>
                    <td class="formValue">
                        <input id="F_FullName" name="F_FullName" type="text" class="form-control required" placeholder="请输入名称" />
                    </td>
                </tr>
            </table>
        </div>
        <div class="col-md-6 col-sm-6 col-xs-12">
            <table>
                <tr>
                    <th class="formTitle">所属机构</th>
                    <td class="formValue">
                        <select id="F_OrganizationId" name="F_OrganizationId" class="form-control"></select>
                    </td>
                </tr>
            </table>
        </div>
        <div class="col-md-6 col-sm-6 col-xs-12">
            <table>
                <tr>
                    <th class="formTitle">联系人</th>
                    <td class="formValue">
                        <input id="F_Contacts" name="F_Contacts" type="text" class="form-control" placeholder="请输入联系人" />
                    </td>
                </tr>
            </table>
        </div>

        <div class="col-md-6 col-sm-6 col-xs-12">
            <table>
                <tr>
                    <th class="formTitle">电话</th>
                    <td class="formValue">
                        <input id="F_TelePhone" name="F_TelePhone" type="text" class="form-control required" placeholder="请输入电话" />
                    </td>
                </tr>
            </table>
        </div>
        <div class="col-md-6 col-sm-6 col-xs-12">
            <table>
                <tr>
                    <th class="formTitle">邮箱</th>
                    <td class="formValue">
                        <input id="F_Email" name="F_Email" type="text" class="form-control required" placeholder="请输入邮箱" />
                    </td>
                </tr>
            </table>
        </div>
        <div class="col-md-6 col-sm-6 col-xs-12">
            <table>
                <tr>
                    <th class="formTitle">微信</th>
                    <td class="formValue">
                        <input id="F_WeChat" name="F_WeChat" type="text" class="form-control required" placeholder="请输入微信" />
                    </td>
                </tr>
            </table>
        </div>
        <div class="col-md-6 col-sm-6 col-xs-12">
            <table>
                <tr>
                    <th class="formTitle">传真</th>
                    <td class="formValue">
                        <input id="F_Fax" name="F_Fax" type="text" class="form-control required" placeholder="请输入传真" />
                    </td>
                </tr>
            </table>
        </div>
        <div class="col-md-12 col-sm-12 col-xs-12">
            <table>
                <tr>
                    <th class="formTitle">地址</th>
                    <td class="formValue">
                        <input id="F_Address" name="F_Address" type="text" class="form-control" placeholder="请输入地址" />
                    </td>
                </tr>
            </table>
        </div>
        <div class="col-md-12 col-sm-12 col-xs-12">
            <table>
                <tr>
                    <th class="formTitle" valign="top" style="padding-top: 5px;">
                        描述
                    </th>
                    <td class="formValue" colspan="3">
                        <textarea id="F_Description" name="F_Descriptions" class="form-control" style="height: 60px;"></textarea>
                    </td>
                </tr>
            </table>
        </div>
    </div>
</form>

<script src="~/Content/js/framework-FormVar.js"></script>
<script src="~/Areas/WmsManage/Content/js/Supplier/Form.js"></script>
<script src="~/Content/js/framework-Form.js"></script>