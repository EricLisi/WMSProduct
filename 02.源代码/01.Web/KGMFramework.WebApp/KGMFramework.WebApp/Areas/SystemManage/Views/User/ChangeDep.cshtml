@{
    ViewBag.Title = "Form";
    Layout = "~/Views/Shared/_Index.cshtml";
}
<link href="~/Content/plugins/select2/select2.min.css" rel="stylesheet" />
<script src="~/Content/plugins/select2/select2.min.js"></script> 

<div style="padding:5px">
    <div class="topPanel">
        <div class="row">
            <div class="col-sm-10">
                <div class="search">
                    <table class="form">
                        <tr>
                            <th class="formTitle">公司/部门</th>
                            <td>
                                <select id="F_OrganizeS" name="F_OrganizeS" class="form-control" style="width: 240px;">
                                    <option value="">==请选择==</option>
                                </select>
                            </td>
                            <th class="formTitle">姓名</th>
                            <td>
                                <input id="txt_keyword" type="text" class="form-control" placeholder="请输入姓名" style="width: 200px;">
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
            <div class="col-sm-2">
                <button id="btn_search" type="button" class="btn  btn-primary"><i class="fa fa-search"></i>查询</button>
            </div>
        </div>
    </div>
    <div class="gridPanel">
        <table id="gridList"></table>
        <div id="gridPager"></div>
    </div>
</div>
<div class="navbar-fixed-bottom" style="background:#eee">
    <div class="row">
        <div class="col-sm-6">
            <form id="form1">
                <table class="form">
                    <tr>
                        <th class="formTitle">公司/部门</th>
                        <td class="formValue">
                            <select id="F_OrganizeId" name="F_OrganizeId" class="form-control require">
                                <option value="">无</option>
                            </select>
                        </td>
                    </tr>
                </table>
            </form>
        </div>
        <div class="col-sm-4">
        </div>
        <div class="col-sm-2" style="margin-top:6px">
            <a id="btn_finish" class="btn btn-primary" onclick="submitForm1()">确定</a>
            <a id="btn_close" class="btn btn-danger" onclick="btn_close()">关闭</a>
        </div>
    </div>
</div>

<script src="~/Content/js/framework-IndexVar.js"></script>
<script src="~/Areas/SystemManage/Content/js/User/ChangeDep.js"></script>
<script src="~/Content/js/framework-Index.js"></script>