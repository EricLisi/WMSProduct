@{
    ViewBag.Title = "Form";
    Layout = "~/Views/Shared/_Form.cshtml";
}

<form id="form1">
    <div style="margin-top: 10px; margin-left: 10px; margin-right: 10px;">
        <div style="padding-top: 20px; margin-right: 30px;">
            <table class="form">
                <tr>
                    <th class="formTitle">上级</th>
                    <td class="formValue">
                        <select id="F_ParentId" name="F_ParentId" class="form-control required">
                            <option value="0">父节点</option>
                        </select>
                    </td>
                    <th class="formTitle">名称</th>
                    <td class="formValue">
                        <input id="F_FullName" name="F_FullName" type="text" class="form-control required" placeholder="请输入名称" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">编码</th>
                    <td class="formValue">
                        <input id="F_EnCode" name="F_EnCode" type="text" class="form-control" />
                    </td>

                    <th class="formTitle">前缀</th>
                    <td class="formValue">
                        <input id="F_Prefix" name="F_Prefix" type="text" class="form-control" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">来源表</th>
                    <td class="formValue">
                        <input id="F_SourceTable" name="F_SourceTable" type="text" class="form-control" />
                    </td>

                    <th class="formTitle">来源类型</th>
                    <td class="formValue">
                        <select id="F_Source" name="F_Source" class="form-control">
                            <option value="">==请选择==</option>
                        </select>
                    </td>

                </tr>
                <tr>
                    <th class="formTitle">出入库类型</th>
                    <td class="formValue">
                        <select id="F_InoutType" name="F_InoutType" class="form-control required">
                            <option value="2">==请选择==</option>
                            <option value="0">出库</option>
                            <option value="1">入库</option>
                        </select>
                    </td>
                    <th class="formTitle">排序</th>
                    <td class="formValue">
                        <input id="F_SortCode" name="F_SortCode" type="text" class="form-control required" placeholder="请输入排序" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">角色类型</th>
                    <td class="formValue">
                        <select id="F_Type" name="F_Type" class="form-control ">
                            <option value="">==请选择==</option>
                        </select>
                    </td>

                    @*<th class="formTitle">归属组织</th>
                    <td class="formValue">
                        <select id="F_OrganizeId" name="F_OrganizeId" class="form-control required"></select>
                    </td>*@
                </tr>
                <tr>
                    <th class="formTitle" style="height: 35px;">选项</th>
                    <td class="formValue" colspan="3" style="padding-top: 1px;">
                        <div class="ckbox">
                            <input id="F_BrbFlag" name="F_BrbFlag" type="checkbox"><label for="F_BrbFlag">红字</label>
                        </div>
                        <div class="ckbox">
                            <input id="F_IsNeedDverfy" name="F_IsNeedDverfy" type="checkbox"><label for="F_IsNeedDverfy">审核</label>
                        </div>
                        <div class="ckbox">
                            <input id="F_IsAllMatch" name="F_IsAllMatch" type="checkbox"><label for="F_IsAllMatch">完全匹配</label>
                        </div>
                        <div class="ckbox">
                            <input id="F_EnabledMark" name="F_EnabledMark" type="checkbox" checked="checked"><label for="F_EnabledMark">有效</label>
                        </div>
                        <div class="ckbox">
                            <input id="F_IsFiFo" name="F_IsFiFo" type="checkbox"><label for="F_IsFiFo">先进先出</label>
                        </div>
                        <div class="ckbox">
                            <input id="F_Bprint" name="F_Bprint" type="checkbox"><label for="F_Bprint">打印</label>
                        </div>
                        <div class="ckbox">
                            <input id="F_Bdefalut" name="F_Bdefalut" type="checkbox"><label for="F_Bdefalut">默认来源</label>
                        </div>
                    </td>
                </tr>

            </table>
            <div class="form-button" id="wizard-actions">
                <a id="btn_finish" class="btn btn-primary" onclick="submitForm1()">完成</a>
                <a id="btn_close" class="btn btn-danger" onclick="btn_close()">关闭</a>
            </div>
        </div>
    </div>
</form>

<script src="~/Content/js/framework-FormVar.js"></script>
<script src="~/Areas/SystemManage/Content/js/VouchTypeSetting/Form.js"></script>
<script src="~/Content/js/framework-Form.js"></script>