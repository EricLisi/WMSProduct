@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Index.cshtml";
}
<div class="topPanel">
    <div class="search">
        <table>
            <tr>
                <td>
                    <div class="input-group">
                        <input id="txt_keyword" type="text" class="form-control" placeholder="产品名称/仓库名称" searchfiled="F_FullName" style="width: 200px;">
                        <span class="input-group-btn">
                            <button id="btn_search" type="button" class="btn  btn-primary"><i class="fa fa-search"></i></button>
                            <button id="btn_search1" type="button" class="btn  btn-primary" onclick="AdvenSearch()">高级查询</button>
                        </span>
                    </div>
                </td>
            </tr>
        </table>
    </div>
    <div class="toolbar">
       
        <div class="btn-group">
            <a class="btn btn-primary" onclick="$.reload()"><span class="glyphicon glyphicon-refresh"></span></a>
        </div>
        <div class="btn-group">
            <a id="NF-OutExcel" class="btn btn-primary dropdown-text" onclick="btn_OutExcel({ router: 'OutRecords' })"><i class="fa fa-plus"></i>导出Excel</a>
        </div>
        <script>$('.toolbar').authorizeButton()</script>
    </div>
</div>
<div class="gridPanel">
    <table id="gridList"></table>
    <div id="gridPager"></div>
</div> 
<script src="~/Content/js/framework-IndexVar.js"></script>
<script src="~/Areas/WmsManage/Content/js/OutRecords/Index.js"></script>
<script src="~/Content/js/framework-Index.js"></script>