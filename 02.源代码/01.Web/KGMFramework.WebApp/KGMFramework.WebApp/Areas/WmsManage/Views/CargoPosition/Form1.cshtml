@{
    ViewBag.Title = "Form";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<form id="form1">
    <div style="padding-top: 20px; margin-right: 20px;">
        <table class="form">
            <tr>
                <th class="formTitle">货位编号前缀</th>
                <td class="formValue">
                    <input id="F_Prefix" name="F_Prefix" type="text" class="form-control required" placeholder="请输入货位编号" />
                </td>
            </tr>
            <tr>
                <th class="formTitle">编号</th>
                <td class="formValue">
                    <input id="F_BenginNum" name="F_BenginNum" type="text" class="form-control required" placeholder="请输入起始数字" style="width:30%;float:left" /><label style="float:left;font-size:20px;">~</label>
                    <input id="F_EndNum" name="F_EndNum" type="text" class="form-control required" placeholder="请输入终止数字" style="width:30%;" />
                </td>
            </tr>
            <tr>
                <th class="formTitle">货位名称</th>
                <td class="formValue">
                    <input id="F_FullName" name="F_FullName" type="text" class="form-control required" placeholder="请输入货位名称" />
                </td>
            </tr>
            <tr>
                <th class="formTitle">单位</th>
                <td class="formValue">
                    <select id="F_Unit" name="F_Unit" class="form-control">
                        <option value="箱">箱</option>
                        <option value="个">个</option>
                    </select>
                </td>
            </tr>
            <tr>
                <th class="formTitle">备注</th>
                <td class="formValue">
                    <input id="F_Description" name="F_Description" type="text" class="form-control" placeholder="备注" />
                </td>
            </tr>
            <tr>
                <th class="formTitle">所属仓库</th>
                <td class="formValue">
                    <select id="F_WarehouseId" name="F_WarehouseId" class="form-control"></select>
                </td>
            </tr>
        </table>
    </div>
</form>
<script src="~/Content/js/framework-FormVar.js"></script>
<script src="~/Areas/WmsManage/Content/js/CargoPosition/Form.js"></script>
<script src="~/Content/js/framework-Form.js"></script>
