@{
    ViewBag.Title = "Form";
    Layout = "~/Views/Shared/_Form.cshtml";
}
 
<form id="form1">
    <div style="margin-top: 10px; margin-left: 10px; margin-right: 10px;">
        <ul class="nav nav-tabs">
            <li role="presentation" class="active"><a href="#">Essential information</a></li>
            @*<li role="presentation"><a href="#">扩展信息</a></li>*@
        </ul>
        <div style="padding-top: 20px; margin-right: 30px;">
            <table class="form">
                <tr>
                    <th class="formTitle"></th>
                    <td class="formValue">
                        <input id="F_Id" name="F_Id" type="hidden" class="form-control" readonly="readonly" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">GtsLicenseNo</th>
                    <td class="formValue">
                        <div id="F_GtsLicenseNo1" style="width:200px"></div>
                        <input id="F_GtsLicenseNo" name="F_GtsLicenseNo" type="hidden" class="form-control" readonly />
                    </td>
                    <th class="formTitle">Material Number</th>
                    <td class="formValue">
                        <input id="F_Material" name="F_Material" type="text" class="form-control required" placeholder="please enter Material Number" />
                    </td>
                </tr>
                @*<tr>
                    <th class="formTitle">GTIN</th>
                    <td class="formValue">
                        <input id="F_Gtin" name="F_Gtin" type="text" class="form-control required" placeholder="please enter GTIN" />
                    </td>
                    <th class="formTitle">Version</th>
                    <td class="formValue">
                        <input id="F_Version" name="F_Version" type="text" class="form-control" readonly="readonly" placeholder="Version Auto produce" />
                    </td>
                </tr>*@
                <tr>
                    @*<th class="formTitle">UOM</th>
                    <td class="formValue">
                        <input id="F_UoM" name="F_UoM" type="text" class="form-control required" placeholder="please enter UOM" />
                    </td>*@
                    <th class="formTitle">Product No</th>
                    <td class="formValue">
                        <input id="F_ProductNo" name="F_ProductNo" type="text" class="form-control required" placeholder="please enter Product No" />
                    </td>

                    <th class="formTitle">BuCode</th>
                    <td class="formValue">
                        <input id="F_BuCode" name="F_BuCode" type="text" class="form-control required" placeholder="please enter BuCode" />
                    </td>
                </tr>
                <tr>
                    
                    <th class="formTitle">BuName</th>
                    <td class="formValue">
                        <input id="F_BuName" name="F_BuName" type="text" class="form-control required" placeholder="please enter BuName" />
                    </td>

                    <th class="formTitle">Define1</th>
                    <td class="formValue">
                        <input id="F_Define1" name="F_Define1" type="text" class="form-control" placeholder="please enter Define1" />
                    </td>
                </tr>
                <tr>
                    
                    <th class="formTitle">Define2</th>
                    <td class="formValue">
                        <input id="F_Define2" name="F_Define2" type="text" class="form-control" placeholder="please enter Define2" />
                    </td>

                    <th class="formTitle">Define3</th>
                    <td class="formValue">
                        <input id="F_Define3" name="F_Define3" type="text" class="form-control" placeholder="please enter Define3" />
                    </td>
                </tr>
                <tr>
                    
                    <th class="formTitle">Define4</th>
                    <td class="formValue">
                        <input id="F_Define4" name="F_Define4" type="text" class="form-control" placeholder="please enter Define4" />
                    </td>

                    <th class="formTitle">Define5</th>
                    <td class="formValue">
                        <input id="F_Define5" name="F_Define5" type="text" class="form-control" placeholder="please enter Define5" />
                    </td>
                </tr>
                <tr>
                    
                    <th class="formTitle">Define6</th>
                    <td class="formValue">
                        <input id="F_Define6" name="F_Define6" type="text" class="form-control" placeholder="please enter Define6" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle" valign="top" style="padding-top: 5px;">MaterialDesc</th>
                    <td class="formValue" colspan="3">
                        <textarea id="F_MaterialDesc" name="F_MaterialDesc" class="form-control required" style="height: 60px;"></textarea>
                    </td>
                </tr>
                <tr>
                    <th class="formTitle" valign="top" style="padding-top: 5px;">MaterialEnDesc</th>
                    <td class="formValue" colspan="3">
                        <textarea id="F_MaterialEnDesc" name="F_MaterialEnDesc" class="form-control" style="height: 60px;"></textarea>
                    </td>
                </tr>
            </table>
        </div>
    </div>
</form>

<script src="~/Content/plugins/jqgrid/jquery.tablednd.js"></script>
<script src="~/Content/js/framework-FormVar.js"></script>
<script src="~/Areas/PrintLabelManage/Content/js/Products/Form.js"></script>
<script src="~/Content/js/framework-Form.js"></script>